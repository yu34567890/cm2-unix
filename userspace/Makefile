

KERNEL_HEADERS ?=
TARGET ?=
TOOLCHAIN ?=
ROOT ?=
STAGING ?=

LIBC = $(ROOT)/libc

all:
	$(MAKE) -C ./libc STAGING=$(STAGING) TOOLCHAIN=$(TOOLCHAIN) KERNEL_HEADERS=$(KERNEL_HEADERS) all
	$(MAKE) -C ./sh STAGING=$(STAGING) TOOLCHAIN=$(TOOLCHAIN) KERNEL_HEADERS=$(KERNEL_HEADERS) LIBC=$(LIBC) all
	$(MAKE) -C ./utils STAGING=$(STAGING) TOOLCHAIN=$(TOOLCHAIN) KERNEL_HEADERS=$(KERNEL_HEADERS) LIBC=$(LIBC) all


clean:
	$(MAKE) -C ./libc STAGING=$(STAGING) TOOLCHAIN=$(TOOLCHAIN) KERNEL_HEADERS=$(KERNEL_HEADERS) clean
	$(MAKE) -C ./sh STAGING=$(STAGING) TOOLCHAIN=$(TOOLCHAIN) KERNEL_HEADERS=$(KERNEL_HEADERS) LIBC=$(LIBC) clean
	$(MAKE) -C ./utils STAGING=$(STAGING) TOOLCHAIN=$(TOOLCHAIN) KERNEL_HEADERS=$(KERNEL_HEADERS) LIBC=$(LIBC) clean






